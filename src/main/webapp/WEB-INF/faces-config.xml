<?xml version="1.0" encoding="utf-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
<!-- 		<resource-bundle> -->
<!-- 			<base-name>com.web.app.MessageResources</base-name> -->
<!-- 			<var>msgs</var> -->
<!-- 		</resource-bundle> -->
	</application>

	<!-- navigation-rule for login.xhtml -->
	<navigation-rule>
		<from-view-id>/public/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/private/messages.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>goout</from-outcome>
			<to-view-id>/public/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>